            "String key_value, out;",
            "for( int i = 0; i < keys.length; i++ ) {",
            "    key_value = keys[i];",
            "    out = String.format( \"%s -> %s\", key_value, env.get( key_value ) );",
            "    System.out.println( out );",
            "}",
            "",
            "// 拡張 for",
            "for( String key_entry : keys ) {",
            "    out = String.format( \"%s -> %s\", key_entry, env.get( key_entry ) );",
            "    System.out.println( out );",
            "}",
            "",
